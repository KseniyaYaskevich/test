{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    // "ecmaFeatures": {
    //   "jsx": true
    // },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  // "plugins": ["prettier"],
  "rules": {
    // "prettier/prettier": "error",
    "no-plusplus": "off",
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    // "indent": ["error", 2],
    "no-console": "error",
    "import/prefer-default-export": 0
  }
}
